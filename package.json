{
  "name": "npm-gui",
  "version": "1.0.7",
  "description": "Graphical User Interface for NPM package.json and dependencies management.",
  "keywords": [
    "gui",
    "npm",
    "yarn",
    "bower",
    "view",
    "client",
    "dependencies",
    "packages",
    "installer",
    "console"
  ],
  "main": "index.js",
  "files": [
    "dist",
    "bin"
  ],
  "scripts": {
    "prepack": "npm run build:server && npm run build:client",
    "dev:server:webpack": "webpack --config webpack.server.config.js --watch --mode=development",
    "dev:server:nodemon": "nodemon --watch dist index.js localhost:9002",
    "dev:server": "concurrently \"npm run dev:server:webpack\" \"npm run dev:server:nodemon\" ",
    "dev:client": "webpack-dev-server --config webpack.client.config.js --hot --mode=development",
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\" ",
    "build:server": "webpack --config webpack.server.config.js --mode=production",
    "build:client": "webpack --config webpack.client.config.js --mode=production",
    "build": "npm run build:server && npm run build:client",
    "test:lint:css": "stylelint client/**/*.vue client/**/*.css",
    "test:lint:js": "eslint \"client/**/**.vue\" \"client/**/**.js\" \"server/actions/**/*.js\" \"server/routers/**/*.js\" ",
    "test:lint": "npm run test:lint:css && npm run test:lint:js",
    "test:unit": "mocha --require @babel/register tests/**.js",
    "test": "npm run test:unit && npm run test:lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/q-nick/npm-gui.git"
  },
  "author": "PaweÅ‚ StefaÅ„ski",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/q-nick/npm-gui/issues"
  },
  "bin": {
    "npm-gui": "./index.js"
  },
  "homepage": "https://github.com/q-nick/npm-gui#readme",
  "devDependencies": {
    "@babel/core": "^7.1.5",
    "@babel/plugin-transform-async-to-generator": "^7.1.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.1.5",
    "@babel/register": "^7.0.0",
    "analytics-node": "^3.3.0",
    "axios": "^0.18.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "body-parser": "^1.18.3",
    "chai": "^4.2.0",
    "concurrently": "^4.0.1",
    "cross-spawn": "^6.0.5",
    "css-loader": "^1.0.1",
    "debounce": "^1.2.0",
    "eslint": "^5.8.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-html": "^4.0.6",
    "eslint-plugin-import": "^2.14.0",
    "express": "^4.16.4",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.6",
    "normalize.css": "^8.0.0",
    "open-iconic": "^1.1.1",
    "opn": "^5.4.0",
    "request": "^2.88.0",
    "rimraf": "^2.6.2",
    "style-loader": "^0.23.1",
    "stylelint": "^9.7.1",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-processor-html": "^1.0.0",
    "supertest": "^3.3.0",
    "url-loader": "^1.1.2",
    "vue": "^2.5.17",
    "vue-loader": "^15.4.2",
    "vue-router": "^3.0.1",
    "vue-template-compiler": "^2.5.17",
    "vuex": "^3.0.1",
    "webpack": "^4.25.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10",
    "webpack-node-externals": "^1.7.2",
    "ws": "^6.1.0"
  },
  "engines": {
    "node": ">= 0.10.0",
    "npm": ">= 1.0.0"
  },
  "dependencies": {}
}
